{
  "name": "سكني",
  "short_name": "سكني",
  "description": "تطبيق سكني للسكن والمواصلات",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#ffffff",
  "id": "com.sakni.app",
  "lang": "ar",
  "dir": "rtl",
  "orientation": "any",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "prefer_related_applications": false,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.sakni.app",
      "id": "com.sakni.app"
    }
  ],
  "scope": "/",
  "scope_extensions": [
    {
      "origin": "https://sakni.app"
    }
  ],
  "categories": ["housing", "transportation"],
  "shortcuts": [
    {
      "name": "السكنات",
      "short_name": "السكنات",
      "description": "عرض قائمة السكنات",
      "url": "/real-estate",
      "icons": [{ "src": "/icons/real-estate.png", "sizes": "192x192" }]
    },
    {
      "name": "الباصات",
      "short_name": "الباصات",
      "description": "عرض قائمة الباصات",
      "url": "/buses",
      "icons": [{ "src": "/icons/buses.png", "sizes": "192x192" }]
    }
  ],
  "icons": [
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/home.png",
      "sizes": "1280x720",
      "type": "image/png"
    },
    {
      "src": "/screenshots/real-estate.png",
      "sizes": "1280x720",
      "type": "image/png"
    },
    {
      "src": "/screenshots/buses.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/pdf": [".pdf"]
      }
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+sakni",
      "url": "/custom-protocol?type=%s"
    }
  ],
  "launch_handler": {
    "client_mode": ["focus-existing", "auto"]
  },
  "display_override": ["window-controls-overlay"],
  "edge_side_panel": {
    "preferred_width": 400
  }
}